{
  "version": "1.2.4@GSS",
  "autoabort": false,
  "DEBUG": {
      "printWarnings": true,
      "printInfos": true
  },
  "useTMPoE": false,
  "socket_msg_size": 65536,
  "mapping_path": "mapping/GssMapping.json",
  "LOGGING": {
    "sensor_log_rate": 100,
    "post_sequence_script": "scripts/plot.sh"
  },
  "CAN" :
  {
    "node_count" : 3,
    "DRIVER" : "SocketCAN",
    "DEVICE": "can0",
    "BUS" :
    {
        "ARBITRATION":
        {
          "bitrate" : 1000000,
          "time_segment_1" : 12,
          "time_segment_2" : 16,
          "sync_jump_width" : 1,
          "no_sampling_points" : 1
        },
        "DATA":
        {
          "bitrate" : 8000000,
          "time_segment_1" : 1,
          "time_segment_2" : 2,
          "sync_jump_width" : 1
        }
    }
  },
  "LLSERVER" :
  {
    "sensor_state_sampling_rate": 10.0
  },
  "WEBSERVER":
  {
    "ip": "127.0.0.1",
    "port": 5555,
    "state_transmission_rate": 15.0,
    "timer_sync_rate": 10,
    "sensors_smoothing_factor": 0.3
  },
  "WARNLIGHT": {
    "ip": "127.0.0.1",
    "port": 25556
  },
  "TMPoE": {
    "ip": "127.0.0.1",
    "port": 25557
  },
  "I2C": {
    "address" : 104
  },
  "Battery": {
    "full": 12600,
    "empty_threshold": 9100,
    "low_threshold": 9900,
    "voltage_hysterisis": 100
  },
  "INFLUXDB":{
    "database_ip": "127.0.0.1",
    "database_port": 8086,
    "database_name": "testDb",
    "debug_measurement": "debug",
    "state_measurement": "states",
    "buffer_size": 2048
  },
  "THRUST":{
    "alpha": 0.785398,
    "beta": 0.785398,
    "gamma": 0.785398,
    "d": 0.28,
    "r": 0.14
  }
}
